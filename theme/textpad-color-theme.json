{
  "$schema": "vscode://schemas/color-theme",
  "name": "TexPad Classic Solarized",
  "tokenColors": [
    {
      "name": "Disable Colors",
      "scope": ["support.constant", "support.function","support.type","keyword.operator.logical",
                "storage.type.function.arrow","entity.name.variable","keyword.operator","storage.modifier.import","meta.method.identifier.java entity.name.function.java",
              "meta.object-literal.key.js", "variable.other",
              "variable.language.rust", "keyword.operator.misc.rust",
                "meta.group.kotlin","meta.block.kotlin","storage.modifier.import.java",
                "variable.parameter.cpp", "entity.name.function","entity.name.type.class"],
      "settings": {
        "foreground": "#073642",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords",
      "scope":["constant.language","keyword.control", "keyword.operator.expression","keyword.other.rust","keyword.other.fn.rust",
              "keyword.operator.sigil","storage.modifier","storage.type.core.rust","storage.type.rust","storage.type.js", "storage.type.function",
              "variable.language.this","keyword.operator.new","keyword.var", "keyword.import", "keyword.struct", 
              "keyword.type","keyword.function","keyword.package", "constant","constant.character","storage.type.ts","keyword.other",
              "keyword.const","keyword.interface","keyword.map","entity.name.tag","keyword.other.new",
              "keyword.operator.kotlin","keyword.operator.misc.rust","keyword.operator.word","storage.type.asp","storage.type.built-in.primitive.c",
              "storage.type.built-in.primitive.cpp","punctuation.definition.tag.end.html","punctuation.definition.tag.begin.html",
              "support.type.primitive.ts","storage.type.def.groovy", "storage.control.clojure",
              "storage.type.class.ts","storage.type.primitive"], //"keyword.operator"
      "settings": {
        //"foreground": "#268BD2",
        //"foreground": "#268bd2",
        //"foreground": "#2f15f9", // "#0000FF",//859900",
        "foreground":"#0000FF"
        //"fontStyle": "bold"
      }
    },
    {
      "name": "Function and Class",
      "scope":["support.class", "entity.name.type",
               "support.constant.core","support.function","meta.function-call.generic", 
               "storage.class.std.rust","support.function.std.rust","support.function.core.rust",
             "support.type.property-name","keyword.operator.asp",
             "support.type.python","storage.type.java","storage.type","support.function.construct","constant.language.register",
             "variable.other.constant.elixir","entity.name.scope-resolution.cpp","entity.name.scope-resolution.function.definition.cpp",
             "meta.method.groovy", "entity.name.function.rust","variable.other.object"],
      "settings": {
        //"foreground": "#0086B3",
        "foreground": "#0000AA", //"#0000AA",
        //"fontStyle": "bold"
      }
    },
    {
      "name": "Numerics",
      "scope":["constant.numeric","constant.language","constant.numeric.decimal.java"],
      "settings": {
        //"foreground": "#cb4b16"
        "foreground": "#cb4b16",
        "fontStyle": ""
       //"foreground": "#839496"
        
      }
    },
    {
      "name": "Numerics",
      "scope":["keyword.operator.arithmetic", "constant.character.escape", "keyword.operator.other.elixir"],
      "settings": {
        //"foreground": "#cb4b16"
        "foreground": "#cb4b16",
        "fontStyle": ""
       //"foreground": "#839496"
        
      }
    },
    {
      "name": "Strings",
      "scope":["punctuation.definition.string.begin","punctuation.definition.string.end","string.quoted","entity.name.import.go","string.regexp"],
      "settings":{
        "foreground": "#008080",
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
         // "foreground": "#93A1A1"
      }
     },
     {
      "name": "Character scape",
      "scope": ["constant.character.escape", "constant.other.rune"],
      "settings": {
          "foreground": "#cb4b16"
      }
     },
     {
      "name": "Comments",
      "scope": ["comment.block","comment.line","punctuation.definition.comment"],
      "settings": {
        "foreground": "#008000",
        "fontStyle": ""
      }
    },
    {
      "name": "Constants",
      "scope": ["constant.other.caps","support.type.exception","constant.other","punctuation.definition.variable.php"],
      "settings": {
        //"foreground": "#DC322F",
        "foreground": "#0000AA",
        "fontStyle": ""
      }
    },
    {
        "name": "HTML",
        "scope":["entity.other.attribute-name.html"],
        "settings": {
            "foreground": "#0000AA",
            "fontStyle": ""
        }
    }
  ],
